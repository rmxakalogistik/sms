  "version": "0.0.0",
  "scripts": {
    "ng-high-memory": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng",
    "ng": "ng",
    "start": "ng serve --ssl true --ssl-cert=ssl\\server.crt --ssl-key=ssl\\server.key",
    "prod": "npm run ng-high-memory -- build --prod",
    "build": "build:dev",
    "build-sw": "build:dev:service-worker",
    "build-pr": "build:prod",
    "build-pr-sw": "build:prod:service-worker",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:dev": "npm run ng:bump-version:dev && ng build",
    "build:dev:service-worker": "npm run ng:bump-version:dev && ng build --service-worker",
    "build:prod": "npm run ng:bump-version:prod && ng build --prod",
    "build:prod:service-worker": "npm run ng:bump-version:prod && ng build --prod --service-worker",
    "ng:bump-version:dev": "gulp ng-bump-version --configuration=staging",
    "ng:bump-version:prod": "gulp ng-bump-version --configuration=prod"
  },
